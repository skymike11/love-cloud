<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="team.seckillgoods.mapper.ActiveGoodsMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="team.seckillgoods.entity.ActiveGoods">
        <id column="active_goods_id" property="activeGoodsId" />
        <result column="daily_goods_id" property="dailyGoodsId" />
        <result column="quanty" property="quanty" />
        <result column="price" property="price" />
        <result column="start_time" property="startTime" />
        <result column="end_time" property="endTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        active_goods_id, daily_goods_id, quanty, price, start_time, end_time
    </sql>

</mapper>
